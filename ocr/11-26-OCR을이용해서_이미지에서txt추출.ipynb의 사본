    "\n",
    "print(\"\\n\" + \"=\" * 80)\n",
    "print(\"ğŸ‰ ì „ì²´ OCR ì‘ì—… ì™„ë£Œ!\")\n",
    "print(\"=\" * 80)\n",
    "print(f\"ğŸ“Š ì²˜ë¦¬ ê²°ê³¼:\")\n",
    "print(f\"   - ì²˜ë¦¬ëœ ì œí’ˆ: {len(product_folders)}ê°œ\")\n",
